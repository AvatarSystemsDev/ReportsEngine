    <DataSet Name="UnitLookup">
      <Query>
        <DataSourceName>Dynamic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pbooUseGroupTypes">
            <Value>=Parameters!pbooUseGroupTypes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@plngCompanyID">
            <Value>=Parameters!plngCompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@plngUserID">
            <Value>=Parameters!plngUserID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @pbooUseGroupTypes = 1
BEGIN
	SELECT
	UnitID AS ID,
	PA_UHD_PAUNIT + ' - ' + PA_UHD_DESC AS Description
	FROM PAUNIT
	LEFT JOIN GroupTypeSelection
	ON GroupTypeSelection.PAUnitID = PAUNIT.UnitID
	WHERE PAUNIT.CompanyID = @plngCompanyID
	AND GroupTypeSelection.PAUnitID IS NOT NULL
	AND GroupTypeSelection.UserID = @plngUserID
	AND GroupTypeSelection.CompanyID = @plngCompanyID
	ORDER BY Description
END
ELSE
BEGIN
	SELECT
	UnitID AS ID,
	PA_UHD_PAUNIT + ' - ' + PA_UHD_DESC AS Description
	FROM PAUNIT
	WHERE PAUNIT.CompanyID = @plngCompanyID
	
	ORDER BY Description
END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AllocationDecimalList">
      <Query>
        <DataSourceName>Dynamic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@plngCompanyID">
            <Value>=Parameters!plngCompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@plngType">
            <Value>=Parameters!plngType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@plngActive">
            <Value>=Parameters!plngActive.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pdteBeginningDate">
            <Value>=Parameters!pdteBeginningDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pdteEndingDate">
            <Value>=Parameters!pdteEndingDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@plngUnitID">
            <Value>=Parameters!plngUnitID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	CASE WHEN PA_ALC_UWTYPE = 'U' AND PA_ALC_LEVEL &gt; 0 THEN PA_ALC_ALLOCID ELSE ISNULL(PA_ALC_PARENT_ID, PA_ALC_ALLOCID) END AS GroupingID,
	CASE WHEN PA_ALC_YEAR = 0 OR PA_ALC_MONTH = 0 THEN 1 ELSE 0 END as IsDefault,
	PA_ALC_ACTIVE as IsActive,
	PA_ALC_ALLOCID as AllocID,
	CASE WHEN PA_ALC_YEAR = 0 OR PA_ALC_MONTH = 0 THEN '1/1/1900' ELSE DATEFROMPARTS(PA_ALC_YEAR, PA_ALC_MONTH, 1) END as SortDate,
	PA_ALC_BASEUnitID as BaseUnitID,
	PA_ALC_UWTYPE as UWType,
	PA_ALC_UnitID as UnitID,
	PA_ALC_WellID as WellID,
	PA_ALC_SEQ as AllocSequence,
	CASE WHEN PA_ALC_UWTYPE = 'U' THEN PAUNIT.PA_UHD_PAUNIT ELSE PAWELL.PA_WEL_PAWELL END as UnitWellNumber,
	CASE WHEN PA_ALC_UWTYPE = 'U' THEN PAUNIT.PA_UHD_DESC ELSE PAWELL.PA_WEL_DESC END AS UnitWellDescription,
	PA_ALC_UALLOC as AllocationBasis,
	PA_ALC_WELVOL as VolumeSource,
	PA_ALC_WELTST as WellTest,
	PA_ALC_BEGINV as BeginningInventory,
	PA_ALC_ENDINV as EndingInventory,
	PA_ALC_OILSALES as OilSales,
	PA_ALC_GASSALES as GasSales,
	PA_ALC_WATER as WaterSales,
	PA_ALC_LEVEL+1 as LevelPlusOne
FROM PAALLOC
LEFT JOIN PAUNIT ON PAUNIT.UnitID = PAALLOC.PA_ALC_UnitID
LEFT JOIN PAWELL ON PAWELL.WellID = PAALLOC.PA_ALC_WellID
WHERE (PAUNIT.CompanyID = @plngCompanyID OR PAWELL.CompanyID = @plngCompanyID)
	AND (CASE WHEN @plngType IN (0,1) THEN (CASE WHEN PA_ALC_YEAR = 0 OR PA_ALC_MONTH = 0 THEN 1 ELSE 0 END) ELSE 2 END) = @plngType --Record Type
AND ((@plngActive = 1 AND PA_ALC_ACTIVE = 'Y') OR (@plngActive = 0 AND PA_ALC_ACTIVE = 'N') OR @plngActive = 2)
	AND ((CASE WHEN PA_ALC_YEAR = 0 OR PA_ALC_MONTH = 0 THEN 1 ELSE 0 END) = 1 
		OR (DATEFROMPARTS(PA_ALC_YEAR, PA_ALC_MONTH, 1) &gt;= @pdteBeginningDate 
			AND DATEFROMPARTS(PA_ALC_YEAR, PA_ALC_MONTH, 1) &lt;= @pdteEndingDate)) --date is &lt;= ending date
AND PAALLOC.PA_ALC_BASEUnitID IN (@plngUnitID)

ORDER BY SortDate, BaseUnitID, AllocSequence</CommandText>
      </Query>
