  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>WITH CTE AS (
    SELECT 
        ID,
       ROW_NUMBER() OVER (PARTITION BY PA_LAR_DISTNO, PA_LAR_FIELDCODE ORDER BY ID) AS RowNumber,
PA_LAR_DISTNO,
PA_LAR_FIELDCODE,
PA_LAR_FIELDNAME,
PA_LAR_OPERCODE,
PA_LAR_OPERNAME,
PA_LAR_ADDRESS,
PA_LAR_CITYSTZIP,
PA_LAR_MONTH,
PA_LAR_YEAR,
PA_LAR_CORRECTED,
PA_LAR_USERNAME,
PA_LAR_USERPHONE,
PA_LAR_USERFAX,
PA_LAR_USEREMAIL,
PA_LAR_PRTDATE,
PA_LAR_CODE,
PA_LAR_OPER,
PA_LAR_FPCODE,
PA_LAR_DESC,
PA_LAR_VOL,
PA_LAR_FILENAME,
CreateDate,
CompanyID,
UserID
    FROM PALAR5DACQ
    WHERE CompanyID = @plngCompanyID
)


SELECT
ID,
PA_LAR_DISTNO,
PA_LAR_FIELDCODE,
PA_LAR_FIELDNAME,
PA_LAR_OPERCODE,
PA_LAR_OPERNAME,
PA_LAR_ADDRESS,
PA_LAR_CITYSTZIP,
PA_LAR_MONTH,
PA_LAR_YEAR,
PA_LAR_CORRECTED,
PA_LAR_USERNAME,
CASE 
    WHEN PA_LAR_USERPHONE IS NULL OR PA_LAR_USERPHONE = '' THEN NULL
    WHEN PA_LAR_USERPHONE NOT LIKE '%[^0-9]%' AND LEN(PA_LAR_USERPHONE) = 10 THEN 
        '(' + LEFT(PA_LAR_USERPHONE, 3) + ') ' + SUBSTRING(PA_LAR_USERPHONE, 4, 3) + '-' + RIGHT(PA_LAR_USERPHONE, 4)
    ELSE 
        PA_LAR_USERPHONE
END AS PA_LAR_USERPHONE,
CASE 
    WHEN PA_LAR_USERFAX IS NULL OR PA_LAR_USERFAX = '' THEN NULL
    WHEN PA_LAR_USERFAX NOT LIKE '%[^0-9]%' AND LEN(PA_LAR_USERFAX) = 10 THEN 
        '(' + LEFT(PA_LAR_USERFAX, 3) + ') ' + SUBSTRING(PA_LAR_USERFAX, 4, 3) + '-' + RIGHT(PA_LAR_USERFAX, 4)
    ELSE 
        PA_LAR_USERFAX
END AS PA_LAR_USERFAX,
PA_LAR_USEREMAIL,
PA_LAR_PRTDATE,
PA_LAR_CODE,
PA_LAR_OPER,
PA_LAR_FPCODE,
PA_LAR_DESC,
PA_LAR_VOL,
PA_LAR_FILENAME,
CreateDate,
CompanyID,
UserID
FROM CTE 
WHERE RowNumber= 1</value>
